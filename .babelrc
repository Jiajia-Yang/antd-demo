{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        [
            "import", {
              "libraryName": "antd",
              "style": "css-loader"
            }
        ],
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose" : true }],
        ["transform-react-jsx"],
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ]
  }